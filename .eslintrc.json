{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "jest": true
  },
  "plugins": ["react", "react-native"],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "react/forbid-prop-types": [0],
    "react/require-default-props": [0],
    "global-require": [0]
  }
}
